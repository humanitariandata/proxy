{
  "multipleApplications": true,
  "allowedDomains": "*",
  "mainPort": 80,
  
  "startHttpProxy": false,
  "httpsTargets": [
    {
      "targetPort": 445,
      "target": "https://localhost",
      "sourcePort": 443,
      "source": "sapplies.rodekruis.nl"
    },
    {
      "targetPort": 444,
      "target": "https://localhost",
      "sourcePort": 443,
      "source": "digidoc.rodekruis.nl"
    }
  ],
  "httpTargets": [
    {
      "targetPort": 82,
      "target": "https://localhost",
      "sourcePort": 80,
      "source": "sapplies.rodekruis.nl"
    },
    {
      "targetPort": 81,
      "target": "https://localhost",
      "sourcePort": 80,
      "source": "digidoc.rodekruis.nl"
    }
  ], 
  "startHttpsProxy": true,
  "sslport": 443,
  "key_file": "./config/cert/rodekruis-key.pem",
  "cert_file": "./config/cert/rodekruis-cert.pem",
  "ca_file": "./config/cert/rodekruis-ca.crt"
}