{
  "multipleApplications": true,
  "allowedDomains": "*",
  "mainPort": 80,
  
  "startHttpProxy": true,
  "options": {  
     "digidoc.rodekruis.nl": "http://localhost:3000",
     "sapplies.rodekruis.nl": "http://localhost:3001",
     "digidoc.rodekruis.nl:443": "https://localhost:444",
     "sapplies.rodekruis.nl:443": "https://localhost:445"
   },

  "startHttpsProxy": true,
  "sslport": 443,
  "key_file": "./config/cert/rodekruis-key.pem",
  "cert_file": "./config/cert/rodekruis-cert.pem",
  "ca_file": "./config/cert/rodekruis-ca.crt"
}